{
  "mcpServers": {
    "review-gate-v2": {
      "command": "/usr/bin/python3",
      "args": ["/app/review-gate-v2/review_gate_v2_mcp.py"],
      "env": {
        "PYTHONPATH": "/app/review-gate-v2",
        "PYTHONUNBUFFERED": "1",
        "REVIEW_GATE_MODE": "cloud_deployment",
        "CLOUD_DEPLOYMENT": "true",
        "LOG_LEVEL": "INFO",
        "TEMP_DIR": "/tmp"
      }
    }
  }
}